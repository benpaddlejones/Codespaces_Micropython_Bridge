{
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": ".extension/node_modules/typescript/lib",
  "eslint.workingDirectories": [
    {
      "pattern": ".extension"
    }
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "workbench.colorTheme": "Default Dark+",
  "workbench.iconTheme": "vs-seti",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "window.restoreWindows": "preserve",
  "python.analysis.extraPaths": [
    "/home/node/.vscode-remote/extensions/benpaddlejones.pico-bridge-1.1.0/emulator/mock/micropython",
    "/home/node/.vscode-remote/extensions/benpaddlejones.pico-bridge-1.1.0/emulator/mock/typings",
    "/home/node/.vscode-remote/extensions/benpaddlejones.pico-bridge-2.0.0/emulator/mock/typings",
    "/home/node/.vscode-remote/extensions/benpaddlejones.pico-bridge-2.0.0/emulator/mock/micropython"
  ]
}
